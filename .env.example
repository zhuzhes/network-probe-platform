# 应用配置
APP_NAME=Network Probe Platform
APP_VERSION=0.1.0
DEBUG=false
HOST=0.0.0.0
PORT=8000

# 数据库配置
DB_HOST=localhost
DB_PORT=5432
DB_NAME=network_probe
DB_USER=postgres
DB_PASSWORD=your_password_here

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# RabbitMQ配置
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_VHOST=/

# 安全配置
SECRET_KEY=your_secret_key_here_change_in_production
JWT_ALGORITHM=HS256
JWT_EXPIRE_MINUTES=30
PASSWORD_MIN_LENGTH=8

# 代理配置
AGENT_ID=
AGENT_NAME=probe-agent
SERVER_URL=wss://your-server.com
SERVER_PORT=8000
AGENT_API_KEY=your_agent_api_key_here
CERT_FILE=
KEY_FILE=

# 监控配置
HEARTBEAT_INTERVAL=30
RESOURCE_REPORT_INTERVAL=60

# 任务配置
MAX_CONCURRENT_TASKS=10
TASK_TIMEOUT=300

# 邮件配置
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_app_password
SMTP_TLS=true

# 地理位置数据库
GEOIP_DB_PATH=data/GeoLite2-City.mmdb

# 日志配置
LOG_LEVEL=INFO
LOG_FORMAT=json