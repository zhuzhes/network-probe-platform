# 应用配置
APP_NAME=Network Probe Platform
APP_VERSION=0.1.0
DEBUG=false
HOST=0.0.0.0
PORT=8000

# 数据库配置
DB_HOST=localhost
DB_PORT=5432
DB_NAME=network_probe
DB_USER=postgres
DB_PASSWORD=your_password_here

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# RabbitMQ配置
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_VHOST=/

# 安全配置
SECRET_KEY=your-secret-key-here-change-in-production
JWT_ALGORITHM=HS256
JWT_EXPIRE_MINUTES=30
PASSWORD_MIN_LENGTH=8

# 代理配置
AGENT_ID=
AGENT_NAME=probe-agent
SERVER_URL=wss://your-server.com
SERVER_PORT=8000
AGENT_API_KEY=your-agent-api-key-here
CERT_FILE=
KEY_FILE=
HEARTBEAT_INTERVAL=30
RESOURCE_REPORT_INTERVAL=60
MAX_CONCURRENT_TASKS=10
TASK_TIMEOUT=300