# Database Configuration
POSTGRES_DB=network_probe
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_postgres_password
POSTGRES_PORT=5432

# Redis Configuration
REDIS_PASSWORD=your_secure_redis_password
REDIS_PORT=6379

# RabbitMQ Configuration
RABBITMQ_USER=admin
RABBITMQ_PASSWORD=your_secure_rabbitmq_password
RABBITMQ_VHOST=network_probe
RABBITMQ_PORT=5672
RABBITMQ_MANAGEMENT_PORT=15672

# Application Configuration
SECRET_KEY=your-very-secure-secret-key-change-this-in-production
DEBUG=false
ENVIRONMENT=production
MANAGEMENT_PORT=8000

# JWT Configuration
JWT_SECRET_KEY=your-jwt-secret-key-change-this-in-production
JWT_ALGORITHM=HS256
JWT_EXPIRE_MINUTES=30

# Email Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-email-password
SMTP_TLS=true

# File Upload Configuration
MAX_UPLOAD_SIZE=10485760

# Agent Configuration
AGENT_ID=agent-001
AGENT_NAME=Production Agent 001
AGENT_TOKEN=your-secure-agent-token
AGENT_REPLICAS=1
TLS_VERIFY=true
HEARTBEAT_INTERVAL=30
MAX_CONCURRENT_TASKS=10
RESOURCE_REPORT_INTERVAL=60

# Logging
LOG_LEVEL=INFO

# Nginx Configuration (if using nginx profile)
HTTP_PORT=80
HTTPS_PORT=443

# Management Platform URL for agents
MANAGEMENT_URL=wss://your-domain.com/ws